{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "incremental": true,
    "tsBuildInfoFile": "data/tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include": ["src/types.d.ts", "src/env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
